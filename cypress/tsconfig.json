{
    "extends": "../tsconfig.json",
    "include": ["./**/*.ts", "../cypress.d.ts"],
    "exclude": [],
    "compilerOptions": {
      "types": ["cypress", "@percy/cypress"],
      "lib": ["es2015", "dom"],
      "isolatedModules": false,
      "allowJs": true,
      "noEmit": true,
    },
    "paths": {
      "@src/hooks/*": ["src/hooks/*"],
      "@src/hooks/useAuthentication": ["cypress/mocks/useAuthenticationMock"]
    }
}